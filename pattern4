package siva8;

import java.util.Scanner;

public class patterns1 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int n;
		Scanner sc=new Scanner(System.in);
		n=sc.nextInt();
		for(int i=1;i<=5;i++){//1
			
			for (int j=1;j<=i;j++){
				System.out.print(j);//5
			}
			System.out.println();
		}

	}

}
